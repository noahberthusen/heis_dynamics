  3.813593698460569*^9}},ExpressionUUID->"8c4efacb-1669-48cc-812a-\
7ff2a1486933"],

Cell[BoxData[
 RowBox[{
  RowBox[{"TrotterEvolve", "[", 
   RowBox[{"tf_", ",", "nt_", ",", "init_"}], "]"}], ":=", 
  RowBox[{"Block", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"dt", "=", 
       RowBox[{"N", "[", 
        RowBox[{"tf", "/", "nt"}], "]"}]}], ",", "UOdd", ",", "UEven", ",", 
      "UTrotter", ",", "\[Psi]trot"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"UOdd", "=", 
      RowBox[{"MatrixExp", "[", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "dt", " ", 
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{"Heis", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{"i", "+", "1"}], ",", "L", ",", "1"}], "]"}]}], "]"}],
             "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", "L", ",", "2"}], "}"}]}], "]"}], "/", 
         "4"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"UEven", "=", 
      RowBox[{"MatrixExp", "[", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "dt", " ", 
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{"Heis", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{"i", "+", "1"}], ",", "L", ",", "1"}], "]"}]}], "]"}],
             "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "2", ",", "L", ",", "2"}], "}"}]}], "]"}], "/", 
         "4"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"UTrotter", "=", 
      RowBox[{"UEven", ".", "UOdd"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Psi]trot", "=", 
      RowBox[{"{", "init", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"\[Psi]trot", ",", 
          RowBox[{"UTrotter", ".", 
           RowBox[{"\[Psi]trot", "[", 
            RowBox[{"[", 
             RowBox[{"-", "1"}], "]"}], "]"}]}]}], "]"}], ";"}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "1", ",", "nt"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Psi]trot", "[", 
      RowBox[{"[", 
       RowBox[{"-", "1"}], "]"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
